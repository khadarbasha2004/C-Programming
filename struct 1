#include <stdio.h>

struct person {
    char name[50];
    int age;
    float height;
};

int main() {
    // create a person
    struct person p;
    sprintf(p.name, "&name");
    p.age = &%d;
    p.height = &height;

    // print the person's information
    printf("Name: %s\n", p.name);
    printf("Age: %d\n", p.age);
    printf("Height: %.2f\n", p.height);

    return 0;
}

